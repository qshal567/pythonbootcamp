class Vehicle:
    def __init__(self, capacity):
        self.capacity = capacity

    def fare(self):
        return self.capacity * 100

class Bus(Vehicle):
    def fare(self):
        
        base_fare = super().fare()
        final_fare = base_fare + (0.10 * base_fare)
        return final_fare

bus = Bus(50)

print("Final fare for the Bus:", bus.fare())